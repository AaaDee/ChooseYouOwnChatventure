{
  "compilerOptions": {
    "target": "es2022",
    "outDir": "./build/",
    "module": "es2022",
    "moduleResolution":"node",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "esModuleInterop": true,
  }
}
